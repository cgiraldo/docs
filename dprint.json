{
  "typescript": {},
  "json": {},
  "markdown": {
    "lineWidth": 80,
    "newLineKind": "lf",
    "textWrap": "maintain",
    "emphasisKind": "underscores",
    "strongKind": "asterisks"
  },
  "toml": {},
  "dockerfile": {},
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "proseWrap": "always"
  },
  "includes": [
    "content/en/docs/*.{md,mdx}",
    "content/**/*.{md,mdx}",
    "*.md"
  ],
  "excludes": [
    "**/node_modules",
    "**/*-lock.json"
  ],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.71.2.wasm",
    "https://plugins.dprint.dev/json-0.15.5.wasm",
    "https://plugins.dprint.dev/markdown-0.14.0.wasm",
    "https://plugins.dprint.dev/toml-0.5.4.wasm",
    "https://plugins.dprint.dev/dockerfile-0.3.0.wasm",
    "https://plugins.dprint.dev/prettier-0.11.0.json@385edfc0e1212b3be6412cda49322ecd62cb38ac71b9b463869bebf23a4767e3"
  ]
}